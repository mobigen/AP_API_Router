{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s %(levelname)s [%(filename)s:%(lineno)d] - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "default"
    }
  },
  "loggers": {
    "root": {
      "level": "DEBUG",
      "handlers": ["console"],
      "propagate": false
    },
    "uvicorn.access": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "sqlalchemy.engine": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    }
  }
}